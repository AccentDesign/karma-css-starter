{
  "name": "karma-css-starter",
  "version": "",
  "description": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "http://github.com/accentdesign/karma-css",
  "author": "",
  "keywords": [],
  "license": "",
  "scripts": {
    "css": "npm-run-all --parallel css-lint* css-compile* --sequential css-prefix* css-minify* fontello-dist*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/style.scss dist/css/style.css",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-prefix": "postcss --config postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/style.min.css dist/css/style.css",
    "fontello-dist": "cpy 'scss/fontello/font/*' dist/font",
    "fontello-install": "fontello-cli install --config scss/fontello/config.json --css scss/fontello/css --font scss/fontello/font",
    "fontello-web": "fontello-cli open --config scss/fontello/config.json",
    "livereload": "livereload dist",
    "watch-css": "nodemon --ignore dist/ -e scss -x \"npm run css\"",
    "watch-css:reload": "npm run livereload & npm run watch-css"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "clean-css-cli": "^4.1.10",
    "cpy-cli": "^1.0.1",
    "fontello-cli": "^0.4.0",
    "karma-css": "git+https://git@github.com/accentdesign/karma-css.git",
    "livereload": "^0.6.3",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
